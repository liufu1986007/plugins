{
  "name": "hudiwriter",
  "parameter": {
    "fileType":   "csv",
    "fileName":   "$writer.tableName",
    "dataxName" : "$writer.dataXName",
    "column": [
      #foreach($c in $writer.cols)
{"name":"$c.name","type":"$c.type","nullable": ${c.nullable}},#end],
"writeMode": "$writer.writeMode",
    "hudiSourceOrderingField": "$writer.sourceOrderingField",
    "hudiRecordkey": "$writer.recordField",
    "hudiPartitionpathField": "$writer.partitionPathField",
    "hudiBatchOp": "$writer.opMode.value",
    "hudiTabType": "$writer.tabType.value",
    "shuffleParallelism": ${writer.shuffleParallelism}
  }
}
