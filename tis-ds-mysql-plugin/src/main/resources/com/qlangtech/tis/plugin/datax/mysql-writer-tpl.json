{
  "name": "mysqlwriter",
  "parameter": {
    "column": [${writer.colsQuotes}],
    "connection":[
      {
        "jdbcUrl":"$writer.jdbcUrl",
        "table": ["$writer.tabName"]
      }
    ],
  #if($writer.preSql)  "preSql": [
         "$writer.preSql"
    ],
#end
  #if($writer.preSql)  "postSql": [
      "$writer.postSql"
   ],
#end
  #if($writer.session)  "batchSize": $writer.batchSize,
#end
    "password": "$writer.password",
  #if($writer.session)  "session": ["$writer.session"],
#end
    "username": "$writer.username",
    "writeMode": "$writer.writeMode"
  }
}