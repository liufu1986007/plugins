  "collectionName": {
    "help": "MonogoDB的集合名。【必填】"
  },
  "column": {
    "help": "MongoDB的文档列名。【必填】"
  },
  "name": {
    "help": "Column的名字。【必填】"
  },
  "type": {
    "help": "Column的类型。【选填】"
  },
